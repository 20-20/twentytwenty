{
	"name": "20 20 Comments",
	"manifest_version": 2,
	"version": "0.0.2",
	"permissions": [
		"tabs",
		"<all_urls>",
		"storage"
	],
	"web_accessible_resources": [
		"*.png",
		"*.ttf",
		"*.woff2",
		"*.woff"
	],
	"content_scripts": [
		{
			"matches":["<all_urls>"],
			"js":[
				"dependencies/jquery.js",
				"sidebar.js",
				"highlight.js"
			],
			"css": [
				"dependencies/bulma.css",
				"annotate.css",
				"dependencies/font-awesome/css/font-awesome.css"
				]
		}
	]
}
